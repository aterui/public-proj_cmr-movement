@article{pettyRestrictedMovementMottled2004,
  title = {Restricted Movement by Mottled Sculpin (Pisces: Cottidae) in a Southern {{Appalachian}} Stream},
  shorttitle = {Restricted Movement by Mottled Sculpin (Pisces},
