@article{jacobHabitatMatchingSpatial2015,
  title = {Habitat Matching and Spatial Heterogeneity of Phenotypes: Implications for Metapopulation and Metacommunity Functioning},
  shorttitle = {Habitat Matching and Spatial Heterogeneity of Phenotypes},
  author = {Jacob, Staffan and Bestion, Elvire and Legrand, Delphine and Clobert, Jean and Cote, Julien},
