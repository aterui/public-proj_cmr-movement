}

@article{thierryInterplayAbioticBiotic2024,
  title = {The Interplay between Abiotic and Biotic Factors in Dispersal Decisions in Metacommunities},
  author = {Thierry, M{\'e}lanie and Cote, Julien and Bestion, Elvire and Legrand, Delphine and Clobert, Jean and Jacob, Staffan},
