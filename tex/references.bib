@article{fronhoferConditiondependentMovementDispersal2015,
  title = {Condition-Dependent Movement and Dispersal in Experimental Metacommunities},
  author = {Fronhofer, Emanuel A. and Klecka, Jan and Meli{\'a}n, Carlos J. and Altermatt, Florian},
  year = {2015},
