@article{blonderPredictingPrioritisingCommunity2024,
  title = {Predicting and {{Prioritising Community Assembly}}: {{Learning Outcomes}} via {{Experiments}}},
  shorttitle = {Predicting and {{Prioritising Community Assembly}}},
  author = {Blonder, Benjamin W. and Lim, Michael H. and Godoy, Oscar},
